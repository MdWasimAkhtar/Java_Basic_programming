<!DOCTYPE hibernate-mapping PUBLIC 
    "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
    "http://www.hibernate.org/dtd/hibernate-mapping-3.0.dtd">

<hibernate-mapping>
       <class name="beans.Student" table="studentRecords" schema="system" >
           <id name="id" column="S_id" />
			<property name="name"  column="S_name"></property>
            <property name="email" column="S_email"></property>
            <property name="marks" column="S_email"></property>
        </class>
</hibernate-mapping>